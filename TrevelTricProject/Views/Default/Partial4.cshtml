@using TrevelTricProject.Models.Classes
@model List<Blog>
<div class="box1-left">
    @foreach (var x in Model)
    {
        <div class="bot-img">
            <img src="@x.BlogImage" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@x.explanation.Substring(0,75)...</p>
        </div>
        <div class="clear"></div>
    }

</div>
