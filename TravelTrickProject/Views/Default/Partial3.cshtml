@using TravelTrickProject.Models.Classes
@model List<Blog>
<div class="padding1">
    <h4>Our Best Places</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
        <div class="bot-img">
            <img src="@x.BlogImage" alt="" />
        </div>
        <div class="bot-desc">
            <p>@x.explanation.Substring(0,80)...</p>
        </div>
        <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial4","Default")
</div>